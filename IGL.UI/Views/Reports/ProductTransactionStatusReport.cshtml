@model IEnumerable<IGL.Core.Entities.Reports.ProductTransactionStatusReport>
@{
    ViewData["Title"] = "Product Trandsaction Status Report";
}

<section class="content-header">
    <h1>
        Product Transaction Status Detail
    </h1>
</section>
<section class="content container-fluid">
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Product Transaction Status Report</h3>
                    </div>
                    <div class="box-body">
                        <table id="IGLDataTable" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Employee Name</th>
                                    <th>Phone</th>
                                    <th>Email Id</th>
                                    <th>Transaction Type</th>
                                    <th>Product Name</th>
                                    <th>Quantity</th>
                                    <th>Unit Price</th>
                                    <th>Total Price</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var data in Model)
                                {
                                    <tr>
                                        <td>
                                            @data.EmployeeName
                                        </td>
                                        <td>
                                            @data.Phone
                                        </td>
                                        <td>
                                            @data.EmailId
                                        </td>
                                        <td>
                                            @data.TransactionType
                                        </td>
                                        <td>
                                            @data.ProductName
                                        </td>
                                        <td>
                                            @data.Quantity
                                        </td>
                                        <td>
                                            @data.UnitPrice
                                        </td>
                                        <td>
                                            
                                        </td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>

</section>


<environment include="Production">
    <script src="~/js/EmployeeDetail/employeeDetail.js"></script>
</environment>
<environment include="Development">
    <script src="~/js/EmployeeDetail/employeeDetail.js"></script>
</environment>
